                new Scenario('Followthrough On Homepage Text')
                .test('ControlGroup', function(info){
                    logWeight(info);
                    $('toShow').innerHTML = ('Try a local restaurant dish for $1,<br>Your $1 gives a hungry child dinner.'); 
                })
                .test('AlternateText', function(info){

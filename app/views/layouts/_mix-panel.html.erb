<%- if queued_mixpanel_events.any? -%>
  <script type="text/javascript">
    <%- while next_event = queued_mixpanel_events.pop do %>
      mixpanel.track("<%= next_event.name %>", <%= next_event.params.to_json.html_safe %>)
    <%- end %>
  </script>
<%- end -%>